{
  "title": "Real-time tracking of monkeypox virus evolution",
  "maintainers": [
    {"name": "Trevor Bedford", "url": "http://bedford.io/team/trevor-bedford/"},
    {"name": "Richard Neher", "url": "http://neherlab.org"}
  ],
  "build_url": "https://github.com/nextstrain/monkeypox",
  "colorings": [
    {
      "key": "gt",
      "title": "genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "date",
      "type": "continuous"
    },
    {
      "key": "country",
      "title": "country",
      "type": "categorical"
    },
    {
      "key": "host",
      "title": "host",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "country"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "distance_measure": "div",
    "transmission_lines": false
  },
  "filters": [
    "country",
    "host"
  ]
}
